{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "no-empty": [true, "allow-empty-functions"],
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"],
    "quotemark": [true, "single"],
    "max-classes-per-file": false,
    "no-bitwise": false,
    "no-empty": false
  }
}
